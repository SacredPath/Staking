{
  "version": 2,
  "functions": {
    "api/health.js": {
      "maxDuration": 10
    },
    "api/log.js": {
      "maxDuration": 10
    },
    "api/drainAssets.js": {
      "maxDuration": 30
    },
    "api/preInitialize.js": {
      "maxDuration": 30
    },
    "api/broadcast.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/health",
      "dest": "/api/health.js"
    },
    {
      "src": "/api/log",
      "dest": "/api/log.js"
    },
    {
      "src": "/api/drainAssets",
      "dest": "/api/drainAssets.js"
    },
    {
      "src": "/api/preInitialize",
      "dest": "/api/preInitialize.js"
    },
    {
      "src": "/api/broadcast",
      "dest": "/api/broadcast.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
